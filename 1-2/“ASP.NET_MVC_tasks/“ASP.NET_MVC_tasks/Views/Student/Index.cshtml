
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>
<table>

    <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Major</th>
        <th>Faculity</th>
    </tr>
    @{ 
        foreach (var item in Model) 
        {<tr><td>@item.Id</td><td>@item.Name</td><td>@item.Major</td><td>@item.Faculity</td></tr>}

    }


</table>
